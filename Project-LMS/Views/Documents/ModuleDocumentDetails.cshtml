@model Project_LMS.Models.Document

@{
    ViewBag.Title = "Details";
}

<h2>Module Document Details</h2>

<div>

    <hr />
    <dl class="dl-horizontal">

        <dt>Modulename</dt>
        <dd>@Html.DisplayFor(model => model.Module.Name)</dd>
        <br />
        <dt>Uploaded by</dt>
        <dd>@Html.DisplayFor(model => model.ApplicationUser.FullName)</dd>

        <dt>@Html.DisplayNameFor(model => model.UploadingTime)</dt>
        <dd>@Html.DisplayFor(model => model.UploadingTime)</dd>
        <br />
        <dt>@Html.DisplayNameFor(model => model.Description)</dt>
        <dd>@Html.DisplayFor(model => model.Description)</dd>

        <dt>@Html.DisplayNameFor(model => model.isHomework)</dt>
        <dd>@Html.DisplayFor(model => model.isHomework)</dd>

        <dt>@Html.DisplayNameFor(model => model.FeedBack)</dt>
        <dd>@Html.DisplayFor(model => model.FeedBack)</dd>

        <br />
        <dt>@Html.DisplayNameFor(model => model.DocumentName)</dt>
        <dd>@Html.DisplayFor(model => model.DocumentName)</dd>

        <dt>@Html.DisplayNameFor(model => model.DocumentFileType)</dt>
        <dd>@Html.DisplayFor(model => model.DocumentFileType)</dd>

    </dl>
</div>
<p>
    @Html.ActionLink("Back", "Edit", "Modules", new { id = Model.ModuleId }, new { @class = "btn btn-primary btn-large" })
</p>
